{
  "name": "magneto",
  "version": "3.0.3",
  "displayName": "Magneto",
  "description": "Scrapes, collects and stores magnet links locally.",
  "license": "MIT",
  "homepage": "https://magneto.crackhead.engineering",
  "bugs": "https://github.com/gergogyulai/magneto/issues",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "dev:chrome": "wxt -b chrome",
    "build": "npm run build:release",
    "build:release": "npm run build:chrome:release && npm run build:firefox:release",
    "build:alpha": "npm run build:chrome:alpha && npm run build:firefox:alpha",
    "build:beta": "npm run build:chrome:beta && npm run build:firefox:beta",
    "build:rc": "npm run build:chrome:rc && npm run build:firefox:rc",
    "build:chrome:release": "wxt zip --mode release -b chrome",
    "build:chrome:alpha": "wxt zip --mode alpha -b chrome",
    "build:chrome:beta": "wxt zip --mode beta -b chrome",
    "build:chrome:rc": "wxt zip --mode rc -b chrome",
    "build:firefox:release": "wxt zip --mode release -b firefox",
    "build:firefox:alpha": "wxt zip --mode alpha -b firefox",
    "build:firefox:beta": "wxt zip --mode beta -b firefox",
    "build:firefox:rc": "wxt zip --mode rc -b firefox",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "postinstall": "wxt prepare",
    "clean": "rm -rf .output .wxt dist"
  },
  "devDependencies": {
    "@tsconfig/svelte": "^5.0.5",
    "@wxt-dev/module-svelte": "^2.0.4",
    "mode-watcher": "^1.1.0",
    "svelte": "^5.39.7",
    "svelte-check": "^4.3.1",
    "svelte-sonner": "^1.0.5",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "wxt": "^0.20.6"
  },
  "dependencies": {
    "@internationalized/date": "^3.9.0",
    "@lucide/svelte": "^0.544.0",
    "@tailwindcss/vite": "^4.1.13",
    "bits-ui": "2.11.4",
    "clsx": "^2.1.1",
    "dexie": "^4.2.0",
    "tailwind-merge": "^3.3.1",
    "tailwind-variants": "^3.1.1",
    "tailwindcss": "^4.1.13",
    "tw-animate-css": "^1.3.8"
  },
  "engines": {
    "node": ">=24.0.0"
  }
}