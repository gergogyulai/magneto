{
  "name": "magneto",
  "version": "2.1.0",
  "displayName": "Magneto",
  "description": "Scrapes, collects and stores magnet links locally.",
  "license": "MIT",
  "homepage": "https://magneto.crackhead.engineering",
  "bugs": "https://github.com/gergogyulai/magneto/issues",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build:nozip": "wxt build --mode release",
    "build:nozip:firefox": "wxt build --mode release -b firefox",
    "build:nozip:alpha": "wxt build --mode alpha",
    "build:nozip:alpha:firefox": "wxt build --mode alpha -b firefox",
    "build:nozip:beta": "wxt build --mode beta",
    "build:nozip:beta:firefox": "wxt build --mode beta -b firefox",
    "build:nozip:rc": "wxt build --mode rc",
    "build:nozip:rc:firefox": "wxt build --mode rc -b firefox",
    "build": "wxt zip --mode release",
    "build:firefox": "wxt zip --mode release -b firefox",
    "build:alpha": "wxt zip --mode alpha",
    "build:alpha:firefox": "wxt zip --mode alpha -b firefox",
    "build:beta": "wxt zip --mode beta",
    "build:beta:firefox": "wxt zip --mode beta -b firefox",
    "build:rc": "wxt zip --mode rc",
    "build:rc:firefox": "wxt zip --mode rc -b firefox",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "postinstall": "wxt prepare",
    "reset": "rm -rf node_modules && rm -rf package-lock.json && rm -rf bun.lock && rm -rf .output && rm -rf .wxt"
  },
  "devDependencies": {
    "@tsconfig/svelte": "^5.0.5",
    "@wxt-dev/module-svelte": "^2.0.3",
    "mode-watcher": "^1.1.0",
    "svelte": "^5.38.6",
    "svelte-check": "^4.3.1",
    "svelte-sonner": "^1.0.5",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "wxt": "^0.20.6"
  },
  "dependencies": {
    "@internationalized/date": "^3.9.0",
    "@lucide/svelte": "^0.544.0",
    "@tailwindcss/vite": "^4.1.13",
    "bits-ui": "^2.11.0",
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.3.1",
    "tailwind-variants": "^3.1.1",
    "tailwindcss": "^4.1.13",
    "tw-animate-css": "^1.3.8"
  }
}
